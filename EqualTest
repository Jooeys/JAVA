package com.zhong;

public class EqualTest {
	
		public static void main(String[]args){
		String s1,s2,s3;
		s1="abc";
		s2="abc";
		System.out.println("s1.equals(s2)="+s1.equals(s2));
		System.out.println("(s1==s2)="+(s1==s2));
		s3=new String(" abc ");
		System.out.println("(s3==s2)="+(s3==s2));
		}
		}
